   "source": [
    "##### ¿Puedes imprimir los dinuleótidos que tengan counts igual a 2?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "b83480e8",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Dinucleótidos con counts = 2:\n",
      "TC : 2\n",
      "CG : 2\n"
     ]
    }
   ],
   "source": [
    "print(\"Dinucleótidos con counts = 2:\")\n",
    "for dinuc, valor in counts.items():\n",
    "    if valor == 2:\n",
    "        print(dinuc, \":\", valor)"
   ]
